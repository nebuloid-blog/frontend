	],
	"packageManager": "yarn@3.2.1",
	"main": "",
	"type": "module",
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"storybook": "storybook dev -s public -p 6006",
		"build-storybook": "storybook build",

		"indent": "replace-in-files --string='  ' --replacement='\t'",

		"generate":
			"yarn generate:code && yarn generate:lint --fix; yarn generate:indent",
		"generate:indent":
			"yarn indent ./src/**/generated/**/*.ts",
		"generate:code":
			"echo generating... && graphql-codegen --config .codegenrc.json",
		"generate:lint":
			"echo linting... && eslint ./src/**/generated/**/*.ts --no-ignore --rule '@typescript-eslint/consistent-type-definitions: 0'"

	},
	"dependencies": {
		"@tanstack/react-query": "^4.16.1",
		"graphql": "^16.6.0",
		"next": "^13.0.2",
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	},
	"devDependencies": {
		"@graphql-codegen/cli": "^2.13.12",
		"@graphql-codegen/client-preset": "^1.1.3",
		"@storybook/addon-essentials": "^7.0.0-alpha.40",
		"@storybook/addon-interactions": "^7.0.0-alpha.40",
		"@storybook/addon-links": "^7.0.0-alpha.40",
		"@storybook/react": "^7.0.0-alpha.40",
		"@storybook/react-webpack5": "^7.0.0-alpha.40",
		"@storybook/testing-library": "^0.0.13",
		"@types/node": "^18.11.9",
		"@types/react": "^18.0.25",
		"@types/react-dom": "^18.0.8",
		"@typescript-eslint/eslint-plugin": "^5.42.1",
		"@typescript-eslint/parser": "^5.42.1",
		"eslint": "^8.27.0",
		"eslint-config-next": "^13.0.2",
		"eslint-config-tabbify-typescript": "^0.1.9",
		"eslint-plugin-import": "^2.26.0",
		"replace-in-files-cli": "^2.0.0",
		"sass": "^1.56.0",
		"storybook": "^7.0.0-alpha.40",
		"storybook-addon-next": "^1.6.10",
		"storybook-addon-swc": "^1.1.9",
		"typescript": "^4.8.4"
	}
}
